  <item>
    <title>Sometimes I wish I were a guitar player</title>
    <link>http://feeds.feedburner.com/~r/mousso/~3/449949584/Sometimes-I-wish-I-were-a-guitar-player</link>
    <guid isPermaLink="false">urn:md5:eb2de3c7683cbd487a4e360aa5277cb5</guid>
    <pubDate>Tue, 11 Nov 2008 21:24:00 +0100</pubDate>
    <dc:creator>Régis</dc:creator>
