public function shuffle_by_splice(arr:Array):Array
{
	var brr:Array;

	brr = new Array();
	while (arr.length&gt;0)
	{
		brr.push(arr.splice(Math.floor(Math.random()*arr.length),1)[0]);
	}

	return brr;
}</description>
