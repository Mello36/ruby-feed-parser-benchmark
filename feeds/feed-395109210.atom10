      <title>CRE085 Nur Fliegen ist schöner</title>
      <author>chaosradio@ccc.de (Chaos Computer Club)</author>
      <itunes:author>Chaos Computer Club</itunes:author>
      <itunes:subtitle>Ein Einblick in die Welt des Flugwesens</itunes:subtitle>
      <description>Ein Einblick in die Welt des Flugwesens</description>
      <enclosure type="audio/mpeg" url="http://chaosradio.ccc.de/archive/chaosradio_express_085.mp3" length="118860852"/>
      <itunes:duration>02:03:42</itunes:duration>
      <guid isPermaLink="false">http://chaosradio.ccc.de/cre085.html-001</guid>
